			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"// Assert created OK",
							"pm.test(\"Created (200/201)\", () => pm.expect([200,201]).to.include(pm.response.code));",
							"",
							"// Parse body safely",
							"let body = {};",
							"try { body = pm.response.json(); } catch (e) { /* non-JSON */ }",
							"",
							"// Handle both response shapes: { id,... } or { todos: [ { id,... } ] }",
							"const item = body?.todos?.[0] || body;",
							"if (item && item.id !== undefined) {",
							"  pm.collectionVariables.set(\"id1\", String(item.id));",
							"  pm.test(\"Captured id1\", () => pm.expect(pm.collectionVariables.get(\"id1\")).to.exist);",
							"} else if (body?.errorMessages) {",
							"  console.warn(\"Creation failed:\", body.errorMessages);",
							"  pm.test(\"No creation error\", () => pm.expect.fail(body.errorMessages.join(\"; \")));",
							"}",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
